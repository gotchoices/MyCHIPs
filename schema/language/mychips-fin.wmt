  {token_ent {Tokenin entiteetti} {Sen entiteetin ID, jolle tämä token on myönnetty}}
  {token_seq {Tokenin järjestysnumero} {Pitää kirjaa erilaisista tämän entiteetin luomista tokeneista}}
  {token {Token} {Tokenin digitaalinen koodi}}
  {reuse {Uudelleenkäytettävä} {Voi myöntää useita tilikirjoja yhdestä tokenista}}
  {used {Käytetty} {Ilmaisee, onko token käytetty}}
  {expires {Vanhenee} {Ilmaisee, milloin token ei ole enää voimassa}}
  {tally_seq {Tilikirjan järjestysnumero} {Indeksi, joka osoittaa, mikä tilikirja myönnetään tämän tokenin yhteydessä}}
  {crt_date {Luotu} {Päivämäärä, jolloin tämä tietue luotiin}}
  {mod_date {Muokattu} {Päivämäärä, jolloin tätä tietuetta viimeksi muokattiin}}
  {crt_by {Luoja} {Käyttäjä, joka loi tämän tietueen}}
  {mod_by {Muokkaaja} {Käyttäjä, joka viimeksi muokkasi tätä tietuetta}}
}

tabtext mychips.tokens_v -lang fin {Käyttäjätokenit} {Standardoitu näkymä käyttäjien yhteystokeneista} {
  {chad {CHIP-osoite} {Tietotietue, joka sisältää CHIP-käyttäjätunnuksen, agentin, isännän ja portin tälle käyttäjälle}}
  {expired {Vanhentunut} {Tosi, jos tokenin voimassaoloaika on päättynyt}}
  {valid {Kelvollinen} {Tosi, jos token on edelleen kelvollinen yhdistämiseen}}
}

