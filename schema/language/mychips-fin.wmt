#Format WMT ver=1

tabtext mychips.routes -lang fin {Ulkomaiset reitit} {Tietoja, jotka on kerätty muilta palvelimilta ulkomaisista vertaisista, joita voidaan tavoittaa vakiintuneiden kirjanpitojen kautta} {
  {rid {Reitin tunnus} {Järjestelmän määräämä ainutlaatuinen kokonaisluku, joka tunnistaa kunkin reittitietueen}}
  {via_ent {Reitin entiteetti} {Paikallisen entiteetin tunnus, joka omistaa kirjanpidon, josta tämä reitti alkaa}}
  {via_tseq {Reitin kirjanpito} {Sen kirjanpidon järjestysnumero, josta tämä reitti alkaa}}
  {dst_cuid {Kohteen CHIP-käyttäjätunnus} {CHIP-tunniste entiteetille, johon tämä reitti johtaa}}
  {dst_agent {Kohteen agentti} {Agentin osoite entiteetille, johon tämä reitti johtaa}}
  {req_ent {Pyytäjä} {Sen paikallisen entiteetin tunnus, jonka kautta tämä reittipyyntö tuli (ja minne kyselyn tulokset palautetaan)}}
  {req_tseq {Pyytäjän kirjanpito} {Sen kirjanpidon järjestysnumero, jonka kautta reittipyyntö tuli (jätä tyhjäksi paikallisille kyselyille)}}
  {status {Tila} {Ilmaisee, onko tämä reitti käytettävissä tai jos ei, mitä edistystä tehdään tällaisen reitin löytämiseksi} {
      {draft {Luonnos} {Reitti on pyydetty, mutta sitä ei ole vielä tarkistettu tai välitetty ylävirtaan}}
      {pend {Odottava} {Pyyntö on tehty ylävirtaan tämän reitin löytämiseksi, mutta lisätilaa ei vielä tunneta paikallisesti}}
      {good {Hyvä} {Ylävirran vertaispalvelin on vastannut, että tämä reitti on mahdollinen}}
      {none {Ei mitään} {Ylävirran vertaispalvelin on vastannut, että tämä reitti ei ole mahdollinen}}
  }}
  {step {Askel} {Kuinka monta askelta olemme edenneet reitillä siitä, mistä tämä reittipyyntö tuli}}
  {lift_count {Käyttölaskuri} {Laskuri, jota kasvatetaan joka kerta, kun tätä polkua käytetään nostossa}}
  {lift_date {Viimeksi käytetty} {Päivämäärä ja aika, jolloin tätä polkua viimeksi käytettiin nostossa}}
  {good_date {Löytymispäivä} {Päivämäärä ja aika, jolloin tämä polku viimeksi merkittiin hyväksi ylävirran ulkomaisen palvelimen toimesta}}
  {min {Noston minimi} {Enimmäismäärä, jonka voimme odottaa nostavamme tätä reittiä pitkin ilman, että vastakappaleita työnnetään niiden tavoiteasetuksen ohi}}
  {max {Noston maksimi} {Enimmäismäärä, jonka voimme odottaa nostavamme tätä reittiä pitkin, mikä saattaa aiheuttaa ylimääräisen tapahtumapalkkion}}
  {margin {Nostomarginaali} {Palkkiosuhde, jota voimme odottaa siirtäessämme mitä tahansa nostotapahtumaa tätä reittiä pitkin}}
  {reward {Nostopalkkio} {Lisäpalkkiosuhde, jota voimme odottaa siirtäessämme nostoja, jotka ylittävät noston minimiarvon}}
} -messages {
  {BST {Virheellinen reitin tila} {Ei kelvollinen tila reitin tilalle}}
  {UNI {Ei ainutlaatuinen reitti} {Reiteillä tulisi olla ainutlaatuinen yhdistelmä lähdettä ja kohdetta}}
  {CRL {Laiton reitin kohde} {Reittien tulisi olla vain käyttäjille muissa järjestelmissä}}
  {LMG {Laiton marginaali} {Marginaalin on oltava määritetty välillä -1 ja 1}}
  {LRW {Laiton palkkio} {Palkkion on oltava määritetty välillä -1 ja 1}}
}

